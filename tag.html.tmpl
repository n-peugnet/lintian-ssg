{{ define "title" }}Lintian Tag: {{ .Name }}{{ end }}

{{ define "description" }}Explanation for the lintian tag {{ .Name }}{{ end }}

{{ define "content" }}
    <h1><code class="{{ .Visibility }}">{{ .Name }}</code></h1>
    {{ .ExplanationHTML }}
    <table>
      <tr>
        <td>Severity: </td>
        <td><code>{{ .Visibility }}</code></td>
      </tr>
      <tr>
        <td>Experimental: </td>
        <td><code>{{ .Experimental }}</code></td>
      </tr>
{{- if .RenamedFrom }}
      <tr>
        <td>Renamed from: </td>
        <td><code>{{ .RenamedFromStr }}</code></td>
      </tr>
{{- end }}
    </table>
{{- if .SeeAlso }}
    <h2>See also</h2>
    <ul id="see-also">
{{- range .SeeAlsoHTML }}
      <li>{{ . }}</li>
{{- end }}
    </ul>
{{- end }}
{{ end }}
